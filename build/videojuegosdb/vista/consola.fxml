<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="videojuegosdb.controlador.ConsolaController">
   <children>
      <Label fx:id="nombre" layoutX="42.0" layoutY="25.0" text="Nombre">
         <font>
            <Font name="Liberation Mono" size="35.0" />
         </font>
      </Label>
      <Label layoutX="43.0" layoutY="124.0" text="Año de lanzamiento:">
         <font>
            <Font name="Liberation Mono" size="23.0" />
         </font>
      </Label>
      <Label layoutX="42.0" layoutY="87.0" text="Compañía:">
         <font>
            <Font name="Liberation Mono" size="23.0" />
         </font>
      </Label>
      <Hyperlink fx:id="compania" layoutX="363.0" layoutY="87.0" onAction="#handleHyperlinkCompania" text="Hyperlink">
         <font>
            <Font name="Liberation Mono" size="23.0" />
         </font>
      </Hyperlink>
      <Label fx:id="anio" layoutX="363.0" layoutY="124.0" text="Label">
         <font>
            <Font name="Liberation Mono" size="23.0" />
         </font>
      </Label>
      <Button alignment="CENTER" layoutX="42.0" layoutY="534.0" mnemonicParsing="false" onAction="#handleBotonElimina" prefHeight="39.0" prefWidth="125.0" style="-fx-background-color: red;" text="Eliminar" textFill="WHITE">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Button>
      <Button alignment="BASELINE_LEFT" layoutX="632.0" layoutY="534.0" mnemonicParsing="false" onAction="#handleBotonRegresa" prefHeight="39.0" prefWidth="125.0" text="Volver">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Button>
      <ImageView disable="true" fitHeight="29.0" fitWidth="33.0" layoutX="700.0" layoutY="540.0">
         <image>
            <Image url="@imagenes/back_arrow.png" />
         </image>
      </ImageView>
      <Button layoutX="42.0" layoutY="322.0" mnemonicParsing="false" onAction="#handleBotonJuegos" prefHeight="71.0" prefWidth="150.0" text="Juegos">
         <font>
            <Font size="26.0" />
         </font>
      </Button>
      <ImageView fitHeight="77.0" fitWidth="92.0" layoutX="214.0" layoutY="322.0">
         <image>
            <Image url="@imagenes/controller.png" />
         </image>
      </ImageView>
      <Button alignment="BASELINE_LEFT" layoutX="545.0" layoutY="534.0" mnemonicParsing="false" onAction="#handleBotonHome" prefHeight="39.0" prefWidth="70.0">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Button>
      <ImageView disable="true" fitHeight="39.0" fitWidth="43.0" layoutX="559.0" layoutY="535.0">
         <image>
            <Image url="@imagenes/home.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
